<?xml version="1.0"?>

<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 6.0.0 EJB//EN" "http://www.bea.com/servers/wls600/dtd/weblogic-ejb-jar.dtd">

<weblogic-ejb-jar>
  <weblogic-enterprise-bean>
    <ejb-name>ItemLocal</ejb-name>

    <entity-descriptor>
      <persistence>
        <persistence-type>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	  <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
	</persistence-type>
	<persistence-use>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	</persistence-use>
      </persistence>

    </entity-descriptor>

    <local-jndi-name>ejb.store.item.ItemLocal</local-jndi-name>

  </weblogic-enterprise-bean>

    <weblogic-enterprise-bean>
    <ejb-name>RackLocal</ejb-name>

    <entity-descriptor>
      <persistence>
        <persistence-type>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	  <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
	</persistence-type>
	<persistence-use>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	</persistence-use>
      </persistence>

    </entity-descriptor>

    <local-jndi-name>ejb.store.rack.RackLocal</local-jndi-name>

  </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
    <ejb-name>PositionLocal</ejb-name>

    <entity-descriptor>
      <persistence>
        <persistence-type>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	  <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
	</persistence-type>
	<persistence-use>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	</persistence-use>
      </persistence>

    </entity-descriptor>

    <local-jndi-name>ejb.store.rack.position.PositionLocal</local-jndi-name>

  </weblogic-enterprise-bean>
    
    <weblogic-enterprise-bean>
    <ejb-name>Store</ejb-name>

    <entity-descriptor>
      <persistence>
        <persistence-type>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	  <type-storage>META-INF/weblogic-cmp-rdbms-jar.xml</type-storage>
	</persistence-type>
	<persistence-use>
	  <type-identifier>WebLogic_CMP_RDBMS</type-identifier>
	  <type-version>6.0</type-version>
	</persistence-use>
      </persistence>

    </entity-descriptor>

    <jndi-name>ejb.store.Store</jndi-name>

  </weblogic-enterprise-bean>
</weblogic-ejb-jar>

