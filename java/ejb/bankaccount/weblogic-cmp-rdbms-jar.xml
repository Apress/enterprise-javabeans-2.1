<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 6.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls600/dtd/weblogic-rdbms-persistence.dtd'>
<weblogic-rdbms-jar>
  <weblogic-rdbms-bean>
    <ejb-name>BankAccount</ejb-name>
    <data-source-name>postgres</data-source-name>
    <table-name>account</table-name>

    <field-map>
      <cmp-field>accountNumber</cmp-field>
      <dbms-column>acno</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>accountDescription</cmp-field>
      <dbms-column>acdesc</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>accountBalance</cmp-field>
      <dbms-column>acbal</dbms-column>
    </field-map>

  </weblogic-rdbms-bean>
</weblogic-rdbms-jar>
