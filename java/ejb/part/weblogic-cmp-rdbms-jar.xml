<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 6.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls600/dtd/weblogic-rdbms-persistence.dtd'>
<weblogic-rdbms-jar>
  <weblogic-rdbms-bean>
    <ejb-name>Part</ejb-name>
    <data-source-name>postgres</data-source-name>
    <table-name>parts</table-name>

    <field-map>
      <cmp-field>partNumber</cmp-field>
      <dbms-column>pn</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>partDescription</cmp-field>
      <dbms-column>pd</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>supplierName</cmp-field>
      <dbms-column>sn</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>price</cmp-field>
      <dbms-column>price</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>lastModified</cmp-field>
      <dbms-column>last_modified</dbms-column>
    </field-map>

  </weblogic-rdbms-bean>
</weblogic-rdbms-jar>
